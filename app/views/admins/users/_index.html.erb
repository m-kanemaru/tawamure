<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th>introduction</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_image_id, size: '100x100',class:'rounded-circle' %></td>
        <td><%= user.nickname %></td>
        <td><%= user.introduction %></td>
        <td><%= link_to 'Show', admins_user_path(user), class: "user_#{user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>