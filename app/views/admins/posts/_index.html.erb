<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% posts.each do |post| %>
      <tr>
        <td><% link_to admins_user_path(post.user) do %>
            <%= image_tag post.user.get_image_id, size:'50x50' %></td>
            <%= post.user.nickname %>
        <% end %></td>
        <td><%= link_to post.text, admins_post_path(post) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
